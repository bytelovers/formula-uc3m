  "scripts": {
    "start": "npm run build:client && node index.js",
    "test": "gulp test",
    "lint": "gulp lint",
    "build:client": "cd ../client && npm run build && rm -rf ../server/src/public && mkdir -p ../server/src/public && cp -R ./dist/* ../server/src/public"
